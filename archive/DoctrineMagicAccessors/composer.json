{
	"name": "kdyby/doctrine-magic-accessors",
	"type": "library",
	"description": "Fast-prototyping magic accessors trait for Doctrine entities",
	"keywords": ["kdyby", "doctrine", "orm", "dbal", "magic", "accessors"],
	"homepage": "http://kdyby.org",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Filip Proch√°zka",
			"homepage": "http://filip-prochazka.com",
			"email": "filip@prochazka.su"
		}
	],
	"support": {
		"email": "filip@prochazka.su",
		"issues": "https://github.com/Kdyby/DoctrineMagicAccessors/issues"
	},
	"require": {
		"php": "^5.6 || ^7.0",
		"kdyby/doctrine-collections-readonly": "^1.0@dev"
	},
	"require-dev": {
		"nette/tester": "~1.7"
	},
	"minimum-stability": "dev",
	"autoload": {
		"psr-4": {
			"Kdyby\\Doctrine\\MagicAccessors\\": "src/"
		},
		"classmap": [
			"src/exceptions.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"KdybyTests\\Doctrine\\MagicAccessors\\": "tests/"
		},
		"classmap": [
			"tests"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	}
}
